{
  "name": "layers-to-x",
  "description": "Quickly turn layers into groups, artboards, and symbols.",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "layers-to-x",
    "manifest": "src/manifest.json",
    "main": "layers-to-x.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asp55/sketch-plugin-layers-to-x.git"
  },
  "author": "Andrew S. Parnell <aparnell@gmail.com>"
}
